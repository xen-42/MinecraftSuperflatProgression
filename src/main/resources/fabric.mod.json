{
	"schemaVersion": 1,
	"id": "superflat-progression",
	"version": "${version}",
	"name": "Superflat Progression",
	"description": "Makes blocks/items/dimensions available in superflat structureless by adding new mobs, crafting, and behaviours",
	"authors": [
		"xen-42"
	],
	"contact": {
		"homepage": "https://github.com/xen-42/MinecraftSuperflatProgression/releases",
		"sources": "https://github.com/xen-42/MinecraftSuperflatProgression",
		"issues": "https://github.com/xen-42/MinecraftSuperflatProgression/issues"
	},
	"license": "MIT",
	"icon": "assets/superflat-progression/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"xen42.superflatprogression.SuperflatProgression"
		],
		"client": [
			"xen42.superflatprogression.SuperflatProgressionClient"
		],
		"fabric-datagen": [
			"xen42.superflatprogression.SuperflatProgressionDataGenerator"
		],
		"rei_client": [
			"xen42.superflatprogression.rei.SuperflatProgressionREIClientPlugin"
		],
		"rei_common": [
			"xen42.superflatprogression.rei.SuperflatProgressionREIServerPlugin"
		]
	},
	"mixins": [
		"superflat-progression.mixins.json",
		{
			"config": "superflat-progression.client.mixins.json",
			"environment": "client"
		}
	],
	"accessWidener": "superflat-progression.accesswidener",
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"roughlyenoughitems": ">=12.1.785"
	}
}